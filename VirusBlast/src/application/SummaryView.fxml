<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="application.SummaryController" spacing="10" alignment="CENTER" style="-fx-padding: 20;">
    <Label fx:id="totalVirusesDefeatedLabel" />
    <Label fx:id="finalScoreLabel" />
    <Label fx:id="highestComboStreakLabel" />
    <Label fx:id="accuracyPercentageLabel" />
    <Label fx:id="timePlayedLabel" />
<Button fx:id="restartButton" text="Restart Game" onAction="#handleRestartButtonAction"/>
<Button fx:id="mainMenuButton" text="Main Menu" onAction="#handleMainMenuButtonAction"/>
</VBox>